{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "headphones.png",
      "default_popup": "popup.html",
      "default_title": "Sign in"
   },
   "content_scripts": [ {
      "js": [ "WILT.js" ],
      "matches": ["*://*.youtube.com/*"],
      "run_at": "document_end"
   } ],
   "description": "Akebia - Command & Control center.",
   "manifest_version": 2,
   "name": "Akebia Streamer Extension",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7"
}
